main
{
    int x, y;

    function int dividir(int a1; int a2) {
        int x, y;

        x = a1 / a2;
        y = -a1 / -a2;

        printf x;
        printf y;

        // x = -2
        // y = -2
        return (-x/-y);
    }

    x = 5/-5;
    y = --20/-10;

    printf x;       //-1
    printf y;       //-2
    printf -y/-x;   //2
    printf y/-x;    //-2

    printf dividir(-20, 10);
}
