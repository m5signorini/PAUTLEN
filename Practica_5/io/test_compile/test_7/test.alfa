main
{
    int x, y;
    boolean b, c;

    function boolean funcionBoolean(boolean a1; boolean a2) {
        int x, y;
        boolean b,c;

        x = 1;
        printf x;

        y = 2;
        printf y;

        //b = true;
        //c = false;
        scanf b;
        scanf c;
        // Imprime true (a1 = true)
        printf (a1 && b);
        // Devuelve false (a2 = false)
        return (a2 || c);
    }

    function int funcionEntera(int a1; int a2) {
        int x, y;
        boolean b,c;

        b = true;
        printf b;

        c = false;
        printf c;

        x = 1;
        y = -1;
        scanf x;
        scanf y;
        // Imprime 4 (a1 = 5)
        printf (a1 - x);
        // Devuelve 5 (a2 = -5)
        return (a2 * y);
    }

    x = 5;
    y = -5;
    b = true;
    c = false;

    printf funcionBoolean(b, c);
    printf funcionEntera(x, y);
}
